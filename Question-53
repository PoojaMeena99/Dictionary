a={1: ['Jean Castro', 'V'], 2: ['Lula Powell', 'V'], 3: ['Brian Howell', 'VI'], 4: ['Lynne Foster', 'VI'], 5:
['Zachary Simon', 'VII']}
dict={}
for i in (a):
    # print(a[i])
    for j in range(len(a[i])):
        # print(a[i][j])
        if j==0:
            p=(a[i][j])
        else:
            s=a[i][j]
    dict.update({p:s})
print(dict)            

